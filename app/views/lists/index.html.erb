<h1>Here are the lists of Movies</h1>
<% @lists.each do |list| %>
  <ul>
    <li><%= list.name %> - <p><%= link_to 'Add bookmarks', new_list_bookmark_path(list), class: 'btn btn-secondary' %></p></li>
  </ul>
<% end %>

<p><%= link_to 'Create a movie list', new_list_path, class: 'btn btn-success' %>
</p>
